<% if (post.tags && post.tags.length) { %>
    <%- list_tags(post.tags, {
        show_count: false,
        class: 'article-tag',
        transform: function (t) {
          var trans = __('tags.' + t);
          if (trans === ('tags.' + t)) return t;
          return trans;
        }
    }) %>
<% } %>